{
  "name": "gatsby-starter-minimal",
  "author": "cafeteando <https://github.com/cafeteando>",
  "description": "A minimal boilerplate starter for Gatsby",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "write test": "echo \"Write tests! -> https://github.com/cafeteando/gatsby-starter-minimal/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.18.12",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cafeteando/gatsby-starter-minimal"
  },
  "bugs": {
    "url": "https://github.com/cafeteando/gatsby-starter-minimal/issues"
  },
  "keywords": [
    "gatsby",
    "minimal"
  ]
}
